{
  "short_name": "To-Do List",
  "name": "KeyCaliber To-Do Extension",
  "description": "A simple extension for adding highlighted text or other things to to-do list",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/checklist16.png",
      "32": "images/checklist32.png",
      "48": "images/checklist48.png",
      "128": "images/checklist128.png"
    }
  },
  "icons": {
    "16": "images/checklist16.png",
    "32": "images/checklist32.png",
    "48": "images/checklist48.png",
    "128": "images/checklist128.png"
  }

}
