{
  "short_name": "To-Do List",
  "name": "KeyCaliber To-Do Extension",
  "description": "A simple extension for adding highlighted text or other things to to-do list",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
      "service_worker": "background.js"
  },
  "permissions": [
      "storage", "tabs", "activeTab", "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo152.png",
      "32": "images/logo152.png",
      "48": "images/logo152.png",
      "128": "images/logo152.png"
    }
  },
  "icons": {
    "16": "images/logo152.png",
    "32": "images/logo152.png",
    "48": "images/logo152.png",
    "128": "images/logo152.png"
  }

}
